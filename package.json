{
  "name": "vue-formic-dynamic-forms",
  "version": "1.0.0",
  "description": "Powerful library for creating dynamic, validated forms in Vue 3 based on JSON schemas",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "README.md",
    "DOCUMENTATION.md"
  ],
  "scripts": {
    "dev": "vite --config vite.config.demo.ts",
    "build": "vite build --config vite.config.lib.ts",
    "build:demo": "vite build --config vite.config.demo.ts",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vue",
    "vue3",
    "forms",
    "dynamic-forms",
    "json-schema",
    "validation",
    "typescript",
    "composables"
  ],
  "author": "Tokimikichika",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "@vue/tsconfig": "^0.5.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  },
  "optionalDependencies": {
    "yup": "^1.0.0",
    "zod": "^3.22.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tokimikichika/Vue-Formic.git"
  },
  "bugs": {
    "url": "https://github.com/Tokimikichika/Vue-Formic/issues"
  },
  "homepage": "https://github.com/Tokimikichika/Vue-Formic#readme"
}